<?php

namespace Fiscalite\GestionFiscaliteBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * Donnee
 *
 * @ORM\Table()
 * @ORM\Entity(repositoryClass="Fiscalite\GestionFiscaliteBundle\Entity\DonneeRepository")
 */
class Donnee
{
    
    /**
   * @ORM\ManyToOne(targetEntity="Fiscalite\GestionFiscaliteBundle\Entity\Ligne", inversedBy="donnees", cascade={"persist"})
   * @ORM\JoinColumn(nullable=false)
   */
    private $ligne;
   /**
   * @ORM\ManyToOne(targetEntity="Fiscalite\GestionFiscaliteBundle\Entity\Colonne", inversedBy="donnees", cascade={"persist"})
   * @ORM\JoinColumn(nullable=false)
   */
    private $colonne;
    /**
     * @var integer
     *
     * @ORM\Column(name="id", type="integer")
     * @ORM\Id
     * @ORM\GeneratedValue(strategy="AUTO")
     */
    private $id;

    /**
     * @var string
     *
     * @ORM\Column(name="valeur", type="string", length=255)
     */
    private $valeur;


    /**
     * Get id
     *
     * @return integer 
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set valeur
     *
     * @param string $valeur
     * @return Donnee
     */
    public function setValeur($valeur)
    {
        $this->valeur = $valeur;
    
        return $this;
    }

    /**
     * Get valeur
     *
     * @return string 
     */
    public function getValeur()
    {
        return $this->valeur;
    }
}
