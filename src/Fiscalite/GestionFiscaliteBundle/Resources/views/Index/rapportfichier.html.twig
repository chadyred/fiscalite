<pdf Author="Direction Finances" Title="Rapport du fichier Document">
    <dynamic-page>
{% block contenu %}
        <placeholders>
            <header>
                <div height="50px" width="100%">
                    <img height="50px" width="80px" src="{{ pdf_image('FiscaliteGestionFiscaliteBundle:logo_haut_de_page.png') }}" />
                </div>
            </header>
        </placeholders>
        <h1>TAXE FONCIÈRE</h1>
        <table>
{% for TFarticleCommuneIFP in ListeTFArticleCommuneIFP %}
            <tr>
                <td text-align="center" padding="3px" >
                    <h5 text-align="center" padding="3px" >Libellé de la commune : {{TFarticleCommuneIFP.getLibCommunne()}}</h5>
                </td>
                <td text-align="center" padding="3px" >
                    <h5 text-align="center" padding="3px" >Libellé intercommunalité à laquelle adhère la commune : {{TFarticleCommuneIFP.getLibelleICAdhC()}}</h5>
                </td>
            </tr>
            <tr>
                <td text-align="center" padding="3px" >
                    <p text-align="center" padding="3px" >Taux bâti/Commune : {{TFarticleCommuneIFP.getTXBC()|number_format(2,',',' ')}}% </p>
                </td>  
                <td text-align="center" padding="3px" >
                    <p text-align="center" padding="3px" >Taux non bâti/Commune : {{TFarticleCommuneIFP.getTXNC()|number_format(2,',',' ')}}% </p>
                </td> 
            </tr>
{% endfor %}
        </table>
        <table>
{% for TFarticleCommuneSRA2 in ListeTFArticleCommuneSRA2 %}
            <tr>
                <td text-align="center" padding="3px" >
                    <p text-align="center" padding="3px" >Total base bâti/Commune : {{TFarticleCommuneSRA2.getTotBBC()|number_format(0,'',' ')}}€ </p>
                </td>  
                <td text-align="center" padding="3px" >
                    <p text-align="center" padding="3px" >Total cotisation bâti/Commune : {{TFarticleCommuneSRA2.getTotCBC()|number_format(0,'',' ')}}€ </p>
                </td> 
            </tr>
{% endfor %}
        </table>
        <table>
{% for TFarticleCommuneSRA3 in ListeTFArticleCommuneSRA3 %}
            <tr>
                <td text-align="center" padding="3px" >
                    <p text-align="center" padding="3px" >Total base non bâti/Commune : {{TFarticleCommuneSRA3.getTotBNC()|number_format(0,'',' ')}}€ </p>
                </td>  
                <td text-align="center" padding="3px" >
                    <p text-align="center" padding="3px" >Total cotisation non bâti/Commune : {{TFarticleCommuneSRA3.getTotCNC()|number_format(0,'',' ')}}€ </p>
                </td> 
            </tr>
{% endfor %}
        </table>
        <h1>TAXE D'HABITATION</h1>
        <table>
{% for THArticleCommune in ListeTHArticleCommune %}
            <tr>
                <td text-align="center" padding="3px" >
                    <p text-align="center" padding="3px" >VLM Commune : {{THArticleCommune.getVlmCne()|number_format(0,'',' ')}} </p>
                </td>  
                <td text-align="center" padding="3px" >
                    <p text-align="center" padding="3px" >VLM ICO : {{THArticleCommune.getVlmICO()|number_format(0,'',' ')}} </p>
                </td> 
                <td text-align="center" padding="3px" >
                    <p text-align="center" padding="3px" >VLM Dept : {{THArticleCommune.getVlmDept()|number_format(0,'',' ')}} </p>
                </td> 
                <td text-align="center" padding="3px"></td>
            </tr>
            <tr>
                <td text-align="center" padding="3px" >
                    <p text-align="center" padding="3px" >Taux d'imposition Commune  : {{THArticleCommune.getTauxImpositionCommune()|number_format(2,',',' ')}}% </p>
                </td>
                <td text-align="center" padding="3px" >
                    <p text-align="center" padding="3px" >Taux d'imposition Syndicats  : {{THArticleCommune.getTauxImpositionSyndicats()|number_format(2,',',' ')}}% </p>
                </td> 
                <td text-align="center" padding="3px" >
                    <p text-align="center" padding="3px" >Taux d'imposition Intercommunalité  : {{THArticleCommune.getTauxImpositionIntercommunalite()|number_format(2,',',' ')}}% </p>
                </td> 
                <td text-align="center" padding="3px" >
                    <p text-align="center" padding="3px" >Taux d'imposition TSE  : {{THArticleCommune.getTauxImpositionTSE()|number_format(2,',',' ')}}% </p>
                </td> 
            </tr>

{% endfor %}
        </table>
        <table>
{% for THArticleCommuneIFPA3 in ListeTHArticleCommuneIFPA3 %}      
            <tr>
                <td text-align="center" padding="3px" >
                    <p text-align="center" padding="3px" >Total Base Nette Commune : {{THArticleCommuneIFPA3.getTotBaseNetteCommune()|number_format(0,'',' ')}} </p>
                </td>  
                <td text-align="center" padding="3px" >
                    <p text-align="center" padding="3px" >Total Base Nette Syndicale : {{THArticleCommuneIFPA3.getTotBaseNetteSyndicale()|number_format(0,'',' ')}} </p>
                </td> 
                <td text-align="center" padding="3px" >
                    <p text-align="center" padding="3px" >Total Base Nette InterCommunalité : {{THArticleCommuneIFPA3.getTotBaseNetteInterCommunale()|number_format(0,'',' ')}} </p>
                </td> 
                <td text-align="center" padding="3px" >
                    <p text-align="center" padding="3px" >Total Base Nette TSE : {{THArticleCommuneIFPA3.getTotBaseNetteTSE()|number_format(0,'',' ')}} </p>
                </td>
            </tr>
            <tr>
                <td text-align="center" padding="3px" >
                    <p text-align="center" padding="3px" >Total Cotisation Commune : {{THArticleCommuneIFPA3.getTotCotisationCommune()|number_format(0,'',' ')}} </p>
                </td>  
                <td text-align="center" padding="3px" >
                    <p text-align="center" padding="3px" >Total Cotisation Syndicale : {{THArticleCommuneIFPA3.getTotCotisationSyndicale()|number_format(0,'',' ')}} </p>
                </td> 
                <td text-align="center" padding="3px" >
                    <p text-align="center" padding="3px" >Total Cotisation InterCommunalité : {{THArticleCommuneIFPA3.getTotCotisationInterCommunale()|number_format(0,'',' ')}} </p>
                </td> 
                <td text-align="center" padding="3px" >
                    <p text-align="center" padding="3px" >Total Cotisation TSE hors GP : {{THArticleCommuneIFPA3.getTotCotisationTSEhorsGP()|number_format(0,'',' ')}} </p>
                </td>
            </tr>
{% endfor %}
        </table>
{% endblock %}
{% block footer %}
        <placeholders>
            <footer>
                <div height="100px" width="100%">
                    <img height="100px" width="100%" src="{{ pdf_image('FiscaliteGestionFiscaliteBundle:logo_bas_de_page.png') }}" />
                </div>
            </footer>
        </placeholders>
{% endblock %}
    </dynamic-page>
</pdf> 